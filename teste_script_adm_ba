#!/bin/bash

#Discovery network components in enterprise environment using fping command for ip address specifics#


fping --interval=5 192.168.1.241 192.168.1.242 192.168.1.245 192.168.1.246 192.168.1.239 192.168.1.243 192.168.1.248 192.168.1.24$
echo ------------------------------------------IP TELEPHONY---------------------------------- >> teste_10
fping --interval=5 192.168.1.38 192.168.1.41 192.168.1.42 192.168.1.43 192.168.1.44 192.168.1.45 192.168.1.46 192.168.1.47 192.16$
echo  -----------------------------------------ACCESS POINT UBIQUITI  ------------------------------------------- >> teste_10
